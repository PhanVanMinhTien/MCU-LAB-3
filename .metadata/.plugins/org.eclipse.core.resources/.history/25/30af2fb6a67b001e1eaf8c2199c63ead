#include "fsm_automatic.h"

void fsm_automatic_run(){
	switch(status){
	case INIT:
		//TODO
		HAL_GPIO_WritePin(LED_RED_GPIO_Port, LED_RED_Pin, GPIO_PIN_SET);
		status = AUTO_RED;
		setTimer1(500);
		break;
	case AUTO_RED:
		break;
	case AUTO_GRE:
		break;
	case AUTO_YEL:
		break;
	default:
		break;
	}
}

